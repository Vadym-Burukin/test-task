{
  "extends": ["airbnb", "prettier", "plugin:react-hooks/recommended"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": ["error"],
    "import/no-extraneous-dependencies": [
      "error",
      {
        "devDependencies": true
      }
    ]
    // "import/resolver": {
    //   "node": {
    //     "extensions": [".js", ".jsx", ".ts", ".tsx"]
    //   }
    // }
  },
  "env": {
    "browser": true,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 2020
  }
}
